traefik.enable=true
traefik.http.services.traefik-tonapi.loadbalancer.server.port=8080
traefik.http.routers.tonapi.rule=Host(`ton-api.dapp-craft.com`)
traefik.http.routers.tonapi.entrypoints=http,https
traefik.http.routers.tonapi.tls=true
traefik.http.routers.tonapi.tls.certresolver=letsEncrypt
traefik.http.middlewares.tonapi.headers.accesscontrolallowmethods=GET,OPTIONS,PUT
traefik.http.middlewares.tonapi.headers.accesscontrolalloworiginlist=*
traefik.http.middlewares.tonapi.headers.accesscontrolmaxage=100
traefik.http.middlewares.tonapi.headers.addvaryheader=true
